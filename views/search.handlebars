<link rel="stylesheet" href="../css/search.css" type="text/css" />

<div id="all-results">
    {{#each this}}
    <a id="{{id}}" class="games" href="#">
    <div id="game-cover">
        <img class="game-posters" src='https:{{cover}}' alt="{{name}}">
    </div>
    <div id="game-info">
        <div id="game-title">
            <p>{{name}}</p>
        </div>
    </div>
    </a>
    {{/each}}
</div>